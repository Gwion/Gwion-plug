#import Nuklear
#import Modules

var Nk.Panel panel;

var Nk.Layout layout;
"test" :=> layout.name;

var Nk.Slider slider;
"test" :=> slider.name;

220 :=> const auto base_freq;
var SinOsc s ~> dac;
spork {
  do {
    base_freq + slider.val * base_freq => s.freq;
    slider => now;
  } while(true);
};

var Nk.Button b;
"Hit me to Quit!" :=> b.name;
b => now;
