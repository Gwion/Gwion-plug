#require Cytosol

123 => const int i;

fun void test(Cytosol.Value values[]) {
   <<< "Print 123: ${i} (checking file global values in callback)" >>>;
   <<< values >>>;
   <<< values.size() >>>;
  foreach(v : values)
    <<< (v $ Cytosol.String) => var string s >>>;
}

var Cytosol cytosol;
cytosol.set_fun("test", test);
cytosol.add_path("test.cyt");
cytosol.compile();
cytosol.run(1);
