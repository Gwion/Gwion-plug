
<faust>
	<name>freeverb</name>
	<author>Grame</author>
	<copyright>(c)GRAME 2006</copyright>
	<license>BSD</license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>2</inputs>
	<outputs>2</outputs>
	<meta key="compile_options">-lang cpp -es 1 -scal -ftz 0</meta>
	<meta key="filename">freeverb.dsp</meta>
	<ui>
		<activewidgets>
			<count>3</count>
			<widget type="hslider" id="1">
				<label>Damp</label>
				<varname>fHslider2</varname>
				<init>0.71999999999999997f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.025000000000000001f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>RoomSize</label>
				<varname>fHslider1</varname>
				<init>0.54000000000000004f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.025000000000000001f</step>
			</widget>
			<widget type="hslider" id="3">
				<label>Wet</label>
				<varname>fHslider0</varname>
				<init>0.14099999999999999f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.025000000000000001f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>Freeverb</label>
				<widgetref id="1" />
				<widgetref id="2" />
				<widgetref id="3" />
			</group>
		</layout>
	</ui>
</faust>
